<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="@android:style/Theme.Light">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="SettingDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowCloseOnTouchOutside">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="SplashTheme" parent="@android:style/Theme.Light">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="ProgressDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/popup</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="clock">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="dialog_button">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="dialpad">
        <item name="android:textSize">@dimen/dialpad_font_size</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>

    <style name="dialpad_ampm" parent="dialpad">
        <item name="android:textSize">@dimen/dialpad_font_size_ampm</item>
    </style>

    <style name="medium_light">
        <item name="android:textSize">@dimen/medium_font_size</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>

    <style name="medium_bold">
        <item name="android:textSize">@dimen/medium_font_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="label">
        <item name="android:textSize">@dimen/label_font_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>

    <style name="alarm_alert" parent="@android:style/Theme.Wallpaper.NoTitleBar"></style>

    <style name="alarm_list_left_column">
        <item name="android:layout_width">68dip</item>
        <item name="android:layout_height">68dip</item>
    </style>

</resources>